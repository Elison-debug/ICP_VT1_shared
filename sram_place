"    set SRAM_DATA_MACRO [dbGet top.insts.name */SRAM_DATA/macro/GENERIC_SRAM_NAME_*_S1P] ; # In this example, all SRAMs are named GENERIC_SRAM_NAME_*_S1P with numbers instead of *
    # i.e., with above, I retrieved a list of my SRAM macros, from which I can access each individual name using the tcl syntax `[lindex $SRAM_DATA_MACRO index]`
    set WEST_OFFSET 200 ; # Example offset in vertical edge
    create_relative_floorplan -place [lindex $SRAM_DATA_MACRO 0] -ref_type core_boundary -bbox both -horizontal_edge_separate {0 10 0} -vertical_edge_separate [list 0 $WEST_OFFSET 0]

    for {set i 1} {$i <= [llength $SRAM_DATA_MACRO]} {incr i} {
        set prev [expr {$i - 1}]
        set idx [expr {$i}]
        create_relative_floorplan -place [lindex $SRAM_DATA_MACRO $i]  -orient R0 -ref_type object -ref [lindex $SRAM_DATA_MACRO $prev] -bbox both -horizontal_edge_separate {1 0 3} -vertical_edge_separate {0 0 0}
        puts ""Placed SRAM_DATA_MACRO($i)""
    }

    # Which gives a result as can be seen above"																						
																						
																						
																						
																						
																						
																						
																						
																						
																						
																						
																						
																						
